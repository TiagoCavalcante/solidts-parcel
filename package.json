{
  "name": "solidts-parcel",
  "version": "1.0.0",
  "description": "SolidJS + TypeScript + Parcel template",
  "repository": {
    "type": "git",
    "url": "https://github.com/TiagoCavalcante/solidts-parcel"
  },
  "scripts": {
    "prepare": "husky install",

    "build": "parcel build src/index.html",
    "start": "parcel src/index.html --open"
  },
  "keywords": [
    "solidjs",
    "typescript",
    "parcel",
    "jsx",
    "template"
  ],
  "author": "Tiago Cavalcante Trindade",
  "license": "MIT",
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx,md}": "prettier --write --no-semi"
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "babel-preset-solid": "^1.3.17",
    "parcel": "^2.5.0",
    "solid-js": "^1.3.17"
  }
}
